{
  "name": "@itrade/dry-run-engine",
  "version": "0.1.0",
  "description": "Dry run engine to simulate strategy execution and persist via TypeORM dry-run entities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && pkgroll --sourcemap --tsconfig tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@itrade/core": "workspace:*",
    "@itrade/data-manager": "workspace:*",
    "decimal.js": "^10.4.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "tsdown": "^0.2.16"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
