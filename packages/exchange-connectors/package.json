{
  "name": "@itrade/exchange-connectors",
  "version": "0.1.0",
  "description": "Exchange connectors with REST and WebSocket support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --noEmit true -p tsconfig.build.json && pkgroll --clean-dist --sourcemap --tsconfig tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@itrade/core": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/ws": "^8.5.10",
    "@types/uuid": "^9.0.8",
    "@types/jsonwebtoken": "^9.0.6",
    "axios": "^1.6.7",
    "ws": "^8.16.0",
    "crypto": "^1.0.1",
    "decimal.js": "^10.4.3",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "tsdown": "^0.2.16"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
